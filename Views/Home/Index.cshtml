        display: flex;
        flex-direction: column;
        margin-left: auto;
        margin-right: auto;
    }

    .info-container {
        display: flex;
        flex-direction: column;
        padding: 48px;
    }

    .info-text {
        color: #1F2937;
        font-size: 36px;
        font-weight: 1000;
        text-align: center;
    }

    .info-image-container {
        display: flex;
        flex-flow: row wrap;
        justify-content: center;
        padding: 48px 0px 80px 0px;
        gap: 48px;
    }

    .info-image {
        display: flex;
        flex-direction: column;
        align-items: center;
        max-width: 150px;
    }

    .info-image-placeholder {
        border: solid #3882F6;
        border-width: 4px;
        border-radius: 12px;
        height: 150px;
        width: 150px;
        margin: 12px;
    }

    .image-subtext {
        color: #1F2937;
        font-weight: 500;
        text-align: center;
    }

    .section-three {
        background-color: #E5E7EB;
    }

    .section-three-container {
        display: flex;
        flex-direction: row;
        justify-content: center;
        align-items: center;
        padding: 100px;
        margin-left: auto;
        margin-right: auto;
    }

    .quote-container {
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
        max-width: 830px;
        gap: 16px;
    }

    .quote-text {
        color: #1F2937;
        font-size: 36px;
        font-weight: 100;
        font-style: italic;
    }

    .quote-source {
        color: #1F2937;
        font-size: 24px;
        font-weight: bold;
        align-self: flex-end;
        padding-right: 32px;
    }

    .section-four {
