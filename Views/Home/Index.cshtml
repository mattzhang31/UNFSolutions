        margin: 0;
        padding: 0;
    }

    a {
        color: #E5E7EB;
        font-size: 18px;
        text-decoration: none;
    }

    ul {
        list-style: none;
    }

    .section-one {
        background-color: #1F2937;
        display: flex;
    }

    .section-one-container {
        display: flex;
        flex-direction: column;
        margin-left: auto;
        margin-right: auto;
    }

    .header,
    .footer {
        color: #F9FAF8;
        background-color: #1F2937;
        display: flex;
        align-items: center;
        padding: 16px;
    }

    .header {
        justify-content: space-between;
        gap: 16px;
    }

    .header-text {
        font-size: 24px;
        font-weight: bold;
        flex-shrink: 1;
    }

    .header-links ul {
        margin: 0;
        padding: 0;
        display: flex;
        gap: 16px;
    }

        .header-links ul li {
            flex-shrink: 1;
        }

    .eyecatch-container {
        background-color: #1F2937;
