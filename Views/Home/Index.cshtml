    }

    .eyecatch-button {
        color: #F9FAF8;
        background-color: #3882F6;
        font-size: 18px;
        font-weight: bold;
        padding: 8px 32px;
        border: none;
        border-radius: 8px;
    }

    .section-two {
        background-color: #F9FAF8;
    }

